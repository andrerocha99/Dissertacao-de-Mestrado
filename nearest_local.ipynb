   "source": [
    "import numpy as np\n",
    "\n",
    "def nearest_local(local,coordinates_data):\n",
    "    distance_to_local = np.sqrt(np.square(coordinates_data-local).sum(axis=1))\n",
    "    ind_local = np.argmin(distance_to_local)\n",
    "    return ind_local, coordinates_data[ind_local]"
   ]
